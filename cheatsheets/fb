[submenu] (12 Reporting)
   [exec] (dradis) {sh -c "service dradis start; xdg-open http://127.0.0.1:3000"}
   [exec] (faraday IDE) {xterm -bg black -fa 'Monospace' -fs 11 -e 'python-faraday ; bash'}
