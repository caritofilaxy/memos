$tmux new -s name 	# start named session
C-b d			# detach session
$tmux attach -t name	# atach session
C-b $			# rename session

C-b "			# split vertically
C-b %			# split horisontally
C-b left,right,up,down  # go to pane
C-b o			# next
C-b x			# quit pane

C-b ?			# help

# windows manage
C-b c			# create new window
C-b ,			# rename windows (uppercase looks more attractive)
C-b {number}		# goto window with number
C-b n			# right shift
C-b p			# left shift
C-b w                   # show windows list
C-b &                   # kill window
